{
    "build": {
        "env": {
            "HUGO_VERSION": "0.139.2",
            "HUGO_ENV": "production",
            "HUGO_ENABLEGITINFO": "true"
        }
    },
    "headers": [
        {
            "source": "(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=86400, max-age=86400"
                },
                {
                    "key": "Vercel-CDN-Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        }
    ]
}